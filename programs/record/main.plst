record Foo
    attr foo : UInt;
    attr bar : UInt;

    constructor new(a : UInt)
        @foo := a;
        @bar := a;
    end

    define plah(a : UInt) -> Bool do
        return a < @bar;
    end
end

define main() -> Bool do
    sys print_bool(Foo.new(6).plah(5));
    return false;
end

service Test()
    constructor new()
    end

    implements EntryPoint
        define main() -> Bool do
            return main();
        end
    end
end

entry
    return Test().new();
end
