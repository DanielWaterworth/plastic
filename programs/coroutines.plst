coroutine foo() Void -> Byte do
    yield 'a';
    return void;
end

define main() -> Bool do
    c := foo();
    sys print_bool(is_done(c));
    sys print_byte(run(c));
    sys print_bool(is_done(c));
    sys print_byte(resume(c, void));
    sys print_bool(is_done(c));
    return true;
end

service Test()
    constructor new()
    end

    implements EntryPoint
        define main() -> Bool do
            return main();
        end
    end
end

entry
    return Test().new();
end
